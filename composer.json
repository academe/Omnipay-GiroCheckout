{
    "name": "academe/omnipay-girocheckout",
    "type": "library",
    "description": "GiroSolution GiroCheckout driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "girosolution",
        "girocheckout"
    ],
    "homepage": "https://github.com/academe/omnipay-girocheckout",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Judge",
            "email": "jason@academe.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\GiroCheckout\\" : "src/"
        }
    },
    "require": {
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^2.8.1"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
